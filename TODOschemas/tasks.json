{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Task",
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "minLength": 1
      },
      "description": {
        "type": "string"
      },
      "dateCreated": {
        "type": "string",
        "format": "date-time"
      },
      "userIdReference": {
        "type": "string"
      },
      "isCompleted": {
        "type": "boolean"
      }
    },
    "required": [
      "title",
      "dateCreated",
      "userIdReference",
      "isCompleted"
    ],
    "additionalProperties": false
  }
  